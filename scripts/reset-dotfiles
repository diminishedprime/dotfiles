# Remove Spacemacs Stuff
rm ~/.spacemacs
# Add Spacemacs
ln -sf ~/dotfiles/.spacemacs ~/.spacemacs

# Remove zsh stuff
rm ~/.zshenv
rm ~/.zshrc
# Add zsh stuff
ln -sf ~/dotfiles/.zshenv ~/.zshenv
ln -sf ~/dotfiles/.zshrc ~/.zshrc

# Remove lein stuff
rm ~/.lein/profiles.clj
# Add lein stuff
ln -sf ~/dotfiles/profiles.clj ~/.lein/profiles.clj
